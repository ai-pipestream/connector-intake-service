{
  "request": {
    "urlPath": "/ai.pipestream.repository.filesystem.upload.NodeUploadService/UploadChunk",
    "method": "POST",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.node_id"
      },
      {
        "matchesJsonPath": "$.upload_id"
      },
      {
        "matchesJsonPath": "$.chunk_number"
      }
    ]
  },
  "response": {
    "status": 200,
    "body": "{\n  \"node_id\": \"{{jsonPath request.body '$.node_id'}}\",\n  \"state\": \"UPLOAD_STATE_UPLOADING\",\n  \"bytes_uploaded\": 0,\n  \"chunk_number\": {{jsonPath request.body '$.chunk_number'}},\n  \"is_file_complete\": {{jsonPath request.body '$.is_last'}}\n}",
    "headers": {
      "grpc-status-name": "OK"
    }
  }
}

